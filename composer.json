{
    "name": "jasny/db",
    "type": "library",
    "description": "Adds modern OOP DB patterns by extending PHP extensions",
    "keywords": ["database", "db"],
    "license": "MIT",
    "homepage": "http://jasny.github.com/db",
    "authors": [
        {
            "name": "Arnold Daniels",
            "email": "arnold@jasny.net",
            "homepage": "http://www.jasny.net"
        }
    ],
    "support": {
        "issues": "https://github.com/jasny/db/issues",
        "source": "https://github.com/jasny/db"
    },
    "require": {
        "php": ">=7.1.0",
        "improved/function": "dev-master",
        "improved/iterable": "dev-master",
        "jasny/php-functions": "dev-master as 4.2.0"
    },
    "require-dev": {
        "jasny/php-code-quality": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Jasny\\DB\\": "src/"
        },
        "files": [
            "src/Option/functions.php",
            "src/Update/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jasny\\DB\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpstan analyse",
            "phpunit",
            "phpcs -p src"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
